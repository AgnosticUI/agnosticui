{
  "name": "agnosticui-react",
  "version": "1.0.0",
  "description": "AgnosticUI (react)",
  "author": "Rob Levin <roblevintennis@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgnosticUI/agnosticui.git"
  },
  "keywords": [
    "agnosticui",
    "agnostic",
    "component",
    "components",
    "ui",
    "react",
    "framework",
    "frontend"
  ],
  "main": "cjs/index.js",
  "module": "module/index.js",
  "browser": "dist/agnosticui-react.js",
  "files": [
    "src/stories",
    "dist",
    "module",
    "cjs"
  ], 
  "scripts": {
    "cjs:rm:extras": "rm -rf cjs/__snapshots__/ && rm cjs/*.stories.js",
    "build:cjs": "NODE_ENV=cjs babel src/stories src/css --out-dir cjs --copy-files --delete-dir-on-start && yarn cjs:rm:extras",
    "module:rm:extras": "rm -rf module/__snapshots__/ && rm module/*.stories.js",
    "build:module": "NODE_ENV=module babel src/stories src/css --out-dir module --copy-files --delete-dir-on-start && yarn module:rm:extras",
    "prepare": "echo 'Building dist | module | cjs...' && yarn build && yarn build:cjs && yarn build:module",
    "build": "webpack -p",
    "build:dev": "webpack --mode development",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "yarn run prettier 'src/**/*.{js,md,html,css,json}' --check",
    "prettier:fix": "yarn prettier:check --write",
    "format": "yarn prettier:fix && yarn lint:fix",
    "pretest": "yarn run format",
    "test": "jest",
    "syncStyles": "node copystyles.js",
    "start": "yarn format && yarn syncStyles && webpack-dev-server --mode development --open",
    "storybook": "yarn format && yarn syncStyles && start-storybook -p 6006",
    "build-storybook": "yarn format && yarn syncStyles && build-storybook"
  },
  "dependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-uuid": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-a11y": "^6.3.8",
    "@storybook/addon-actions": "^6.0.20",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/addon-links": "^6.0.20",
    "@storybook/addon-storyshots": "^6.0.20",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/react": "^6.0.20",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.5",
    "css-loader": "^4.2.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.25.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "prettier": "^2.2.1",
    "react-is": "^16.13.1",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
