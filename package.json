{
  "name": "agnosticui",
  "version": "1.0.3-3",
  "author": "Rob Levin",
  "description": "An agnostic UI component library prioritizing clean HTML and CSS, but built to agnostically work with many popular JavaScript frameworks like React, Vue, and Svelte.",
  "license": "Apache-2.0",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/AgnosticUI/agnosticui.git"
  },
  "scripts": {
    "sync:react": "yarn workspace agnosticui-react run syncStyles",
    "sync:vue": "yarn workspace agnosticui-vue run syncStyles",
    "sync:svelte": "yarn workspace agnosticui-svelte run syncStyles",
    "test:react": "yarn workspace agnosticui-react run test",
    "test:vue": "yarn workspace agnosticui-vue run test",
    "test:svelte": "yarn workspace agnosticui-svelte run test",
    "test": "yarn sync:react && yarn test:react && yarn sync:vue && yarn test:vue && yarn sync:svelte && yarn test:svelte",
    "start:css": "yarn workspace agnosticui-css run start",
    "build:css": "yarn workspace agnosticui-css run build",
    "start:react": "yarn workspace agnosticui-react run storybook",
    "start:vue": "yarn workspace agnosticui-vue run storybook",
    "start:svelte": "yarn workspace agnosticui-svelte run storybook",
    "start:showcase": "yarn workspace showcase run serve"
  },
  "workspaces": [
    "agnosticui-react",
    "agnosticui-svelte",
    "agnosticui-vue",
    "agnosticui-css",
    "showcase"
  ],
  "keywords": [
    "ui components",
    "design system",
    "components",
    "component library"
  ],
  "bugs": {
    "url": "https://github.com/roblevintennis/agnosticui/issues"
  },
  "homepage": "https://github.com/roblevintennis/agnosticui#readme"
}