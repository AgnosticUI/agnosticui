{
  "name": "agnosticui-react-ts",
  "version": "0.1.0",
  "description": "AgnosticUI (react)",
  "author": "Rob Levin <roblevintennis@gmail.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgnosticUI/agnosticui.git"
  },
  "keywords": [
    "agnosticui",
    "agnostic",
    "component",
    "components",
    "ui",
    "react",
    "framework",
    "frontend"
  ],
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/umd/index.js",
  "files": [
    "src/**/*.{css,js,jsx,ts,tsx}",
    "lib/umd/**/*.{css,js,jsx,ts,tsx}",
    "lib/esm/**/*.{css,js,jsx,ts,tsx}",
    "lib/cjs/**/*.{css,js,jsx,ts,tsx}"
  ],
  "scripts": {
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "cp:css": "cp ./src/css/common.min.css ./lib/common.min.css",
    "clean": "rimraf ./lib",
    "prebuild": "yarn clean",
    "prepare": "echo 'Building packages: (umd) esm(module) cjs/(node)' && yarn build && yarn build-storybook",
    "build": "yarn build:esm && yarn build:cjs && yarn build:umd && yarn cp:css",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "build:umd": "tsc -p tsconfig-umd.json",
    "type:check": "tsc -p tsconfig-check.json",
    "type:watch": "tsc -w -p tsconfig-check.json",
    "jest": "./node_modules/jest/bin/jest.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "yarn lint --fix",
    "prettier:check": "yarn run prettier 'src/**/*.{ts,tsx,md,html,css,json}' --check",
    "prettier:fix": "yarn prettier:check --write",
    "format": "yarn prettier:fix && yarn lint:fix",
    "syncStyles": "node copystyles.js",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "yarn format && yarn syncStyles && build-storybook --docs -o docs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@storybook/addon-essentials": "^6.4.0-beta.1",
    "@storybook/addon-links": "^6.4.0-beta.1",
    "@storybook/node-logger": "^6.4.0-beta.1",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.0-beta.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.2",
    "uuid": "^8.3.2",
    "web-vitals": "^1.0.1"
  }
}
