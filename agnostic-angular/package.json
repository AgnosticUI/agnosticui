{
  "name": "agnostic-angular",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "nx": "npx nx",
    "cp:css": "cp ./libs/ag/styles/*.css ./dist",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "echo \"Running the /examples app\" && yarn syncStyles && nx serve --port 4234",
    "build:ag": "yarn syncStyles && nx run ag:build && yarn cp:css",
    "test": "nx test",
    "sync:verify": "yarn syncStyles && yarn lint && yarn format",
    "syncStyles": "node copystyles.js && yarn cp:css",
    "g:lib": "nx generate @nrwl/angular:lib",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "nx:help": "nx help",
    "storybook": "nx run ag:storybook",
    "build-storybook": "yarn sync:verify && nx run ag:build-storybook",
    "e2e:lib": "nx run ag-e2e:e2e"
  },
  "dependencies": {
    "@angular/animations": "13.1.3",
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/forms": "13.1.3",
    "@angular/platform-browser": "13.1.3",
    "@angular/platform-browser-dynamic": "13.1.3",
    "@angular/router": "13.1.3",
    "@nrwl/angular": "13.4.6",
    "agnostic-helpers": "^1.0.2",
    "angular-a11y-dialog": "^0.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.4",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "13.1.4",
    "@angular/compiler-cli": "13.1.3",
    "@angular/language-service": "13.1.3",
    "@nrwl/cli": "13.4.6",
    "@nrwl/cypress": "13.4.6",
    "@nrwl/eslint-plugin-nx": "13.4.6",
    "@nrwl/jest": "13.4.6",
    "@nrwl/linter": "13.4.6",
    "@nrwl/storybook": "13.4.6",
    "@nrwl/tao": "13.4.6",
    "@nrwl/workspace": "13.4.6",
    "@storybook/addon-a11y": "^6.4.14",
    "@storybook/addon-essentials": "6.4.13",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/angular": "6.4.13",
    "@storybook/builder-webpack5": "6.4.13",
    "@storybook/manager-webpack5": "6.4.13",
    "@types/jest": "27.0.2",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.3.1",
