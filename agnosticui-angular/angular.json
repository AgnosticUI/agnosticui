{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "src/stories",
  "projects": {
    "ag": {
      "projectType": "library",
      "root": "src/stories/ag",
      "sourceRoot": "src/stories/ag/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "src/stories/ag/tsconfig.lib.json",
            "project": "src/stories/ag/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "src/stories/ag/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/stories/ag/src/test.ts",
            "tsConfig": "src/stories/ag/tsconfig.spec.json",
            "karmaConfig": "src/stories/ag/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/stories/ag/**/*.ts",
              "src/stories/ag/**/*.html"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ag",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
