    "./Breadcrumb": "./src/lib/components/Breadcrumb.astro",
    "./Button": "./src/lib/components/Button.astro",
    "./Card": "./src/lib/components/Card.astro",
    "./Close": "./src/lib/components/Close.astro",
    "./Dialog": "./src/lib/components/Dialog.astro",
    "./Disclose": "./src/lib/components/Disclose.astro",
    "./Divider": "./src/lib/components/Divider.astro",
    "./EmptyState": "./src/lib/components/EmptyState.astro",
    "./Header": "./src/lib/components/Header.astro",
    "./HeaderNav": "./src/lib/components/HeaderNav.astro",
    "./HeaderNavItem": "./src/lib/components/HeaderNavItem.astro",
    "./Icon": "./src/lib/components/Icon.astro",
    "./Input": "./src/lib/components/Input.astro",
    "./Loader": "./src/lib/components/Loader.astro",
    "./Menu": "./src/lib/components/Menu.astro",
    "./MenuItem": "./src/lib/components/MenuItem.astro",
    "./Progress": "./src/lib/components/Progress.astro",
    "./Select": "./src/lib/components/Select.astro",
    "./Spinner": "./src/lib/components/Spinner.astro",
    "./Switch": "./src/lib/components/Switch.astro",
    "./Tabs": "./src/lib/components/Tabs.astro",
    "./Tag": "./src/lib/components/Tag.astro",
